# IDSE Connection Configuration
# Copy this file to .env and fill in your actual values

# PostgreSQL Database Connection
# Use the same connection string as payjob-platform-v3
PAYJOB_DB_URI=postgresql://user:password@localhost:5432/paytime_strapi

# Strapi URL (for file access)
STRAPI_URL=https://your-strapi-domain.com

# Optional: Direct IDSE credentials (for testing without DB)
IDSE_USER=your_idse_username_here
IDSE_PASSWORD=your_idse_password_here
IDSE_PFX_URL=https://your-domain.com/path/to/your-certificate.pfx